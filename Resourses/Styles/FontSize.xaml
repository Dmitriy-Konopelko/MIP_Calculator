<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"                    
                     xmlns:system="clr-namespace:System;assembly=mscorlib">
    <!--Добавлена библиотека xmlns:system="clr-namespace:System;assembly=mscorlib" для system:Double иначе будет ругаться на неизвестный тип-->    
    <system:Double x:Key="SmallFontSize">10</system:Double>
    <system:Double x:Key="NormalFontSize">12</system:Double>
    <system:Double x:Key="MenuFontSize">14</system:Double>
    <system:Double x:Key="BigFontSize">22</system:Double>
    <system:Double x:Key="TitleFontSize">34</system:Double>
</ResourceDictionary>